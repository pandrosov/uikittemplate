{
    "name": "uikittemplate",
    "title": "UIkit template for website UI",
    "description": "",
    "version": "1.0.0",
    "main": "dist/js/uikit.js",
    "style": "dist/css/uikit.css",
    "sideEffects": [
        "*.css",
        "./src/js/*.js",
        "./dist/**/*.js"
    ],
    "scripts": {
        "build-scss": "node build/scss",
        "compile": "pnpm compile-less && pnpm compile-js",
        "compile-js": "node build/build",
        "compile-less": "pnpm icons && node build/less",
        "compile-rtl": "pnpm compile-less rtl",
        "icons": "node build/icons",
        "prefix": "node build/prefix",
        "scope": "node build/scope",
        "release": "node build/release",
        "watch-dev": "grunt js && grunt imports && grunt images && grunt watch",
        "copy-theme": "grunt copy-theme",
        "watch": "watch-run -i -p \"**/*.less\" -- node build/less",
        "eslint-fix": "eslint --fix src/js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uikit/uikittemplate.git"
    },
    "bugs": {
        "url": "https://github.com/uikit/uikittemplate/issues"
    },
    "homepage": "https://flario.by",
    "packageManager": "pnpm@7.29.1",
    "devDependencies": {
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-replace": "^5.0.2",
        "archiver": "^5.3.1",
        "camelcase": "^7.0.1",
        "clean-css": "^5.3.2",
        "dateformat": "^5.0.3",
        "del": "^7.0.0",
        "esbuild": "^0.17.19",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "fs": "0.0.1-security",
        "fs-extra": "^11.1.1",
        "glob": "^10.2.4",
        "grunt": "^1.6.1",
        "grunt-contrib-clean": "^2.0.1",
        "grunt-contrib-concat": "^2.1.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-imagemin": "^4.0.0",
        "grunt-contrib-less": "^3.0.0",
        "grunt-contrib-uglify": "^5.2.2",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-less-imports": "^1.2.2",
        "grunt-webp": "^0.0.4",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-import-less": "^0.9.0",
        "gulp-inject": "^5.0.5",
        "gulp-less": "^5.0.0",
        "gulp-less-import": "^1.0.0",
        "inquirer": "^9.2.3",
        "less": "^4.1.3",
        "minimist": "^1.2.8",
        "number-precision": "^1.6.0",
        "p-limit": "^4.0.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "rollup": "^3.22.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-modify": "^3.0.0",
        "rtlcss": "^4.1.0",
        "semver": "^7.5.1",
        "svgo": "^3.0.2",
        "watch-run": "^1.2.5",
        "webp-bin": "^0.1.4"
    }
}
